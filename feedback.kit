<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>Mishlen</title>
    <meta name="description" content="" />
    <meta name="author" content="" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />

    <link rel="stylesheet" href="css/style.css" />

    <script src="http://modernizr.com/downloads/modernizr-2.5.3.js"></script>
</head>

<body>

    <div class="page-wrap">

        <!-- @include "blocks/_header.html" -->

        <div class="page-content c">
            <div class="page-content__michelin"></div>

            <div class="page-content__inner">
                <div class="page-content__breadcrumbs">
                    <a href="">Главная</a> /
                    <a href="">Обратная связь</a>
                </div>

                <div class="page-content__heading">Обратная связь</div>

                <form action="" class="feedback-form">
                    <div class="form-row">
                        <label for="">Тема сообщения</label>
                        <input type="text" class="big">
                    </div>

                    <div class="form-row">
                        <label for="">Категория</label>
                        <select name="" class="js-customselect big">
                            <option value="">Не указано</option>
                            <option value="">Указано</option>
                        </select>
                    </div>

                    <div class="form-row">
                        <label for="">Ваше сообщение</label>
                        <textarea name="" class="big"></textarea>
                        <div class="form-hint mt5">Максимальное количество символов 1500</div>
                    </div>

                    <div class="form-row">
                        <label for="">Имя</label>
                        <input type="text">
                    </div>
                    <div class="form-row">
                        <label for="">Телефон</label>
                        <input type="text">
                    </div>
                    <div class="form-row">
                        <label for="">Адрес электронной почты (E-mail)</label>
                        <input type="text">
                    </div>

                    <div class="page-content__hr"></div>

                    <div class="page-content__buttons c">
                        <a href="" class="btn btn_arr l">Отправить</a>
                    </div>

                </form> <!-- /.feedback-form -->
            </div> <!-- /.page-content__inner -->
        </div> <!-- /.page-content -->

        <!-- @include "blocks/_footer.html" -->
        
    </div>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.0.1/jquery.min.js"></script>
    <script defer src="js/script-ck.js"></script>
    
</body>
</html>
